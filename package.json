{
  "name": "@ts-defold/types",
  "version": "1.2.74",
  "description": "TypeScript definitions for Defold",
  "repository": "github:ts-defold/types",
  "keywords": [
    "typescript",
    "lua",
    "defold"
  ],
  "author": "Justin Walsh (https://thejustinwalsh.com/)",
  "license": "MIT",
  "types": "index.d.ts",
  "files": [
    "**/*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm run check",
    "build": "xtgen -m global -o ./",
    "check": "tsc --noEmit"
  },
  "ts-defold": {
    "channel": "stable",
    "version": "latest"
  },
  "dependencies": {
    "lua-types": "^2.10.1"
  },
  "devDependencies": {
    "tsd-ext-type-gen": "^2.0.0",
    "typescript": "5.8.2",
    "typescript-to-lua": "~1.31.0"
  },
  "peerDependencies": {
    "typescript-to-lua": "^1.10.0"
  }
}
